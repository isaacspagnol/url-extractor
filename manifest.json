{
  "manifest_version": 3,
  "name": "URL Extractor",
  "version": "1.0",
  "description": "Extract paths from URLs",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
